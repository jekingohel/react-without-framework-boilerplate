{
  "name": "react-app-without-framework",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "webpack serve --env local",
    "dev": "webpack serve --env development",
    "production": "webpack serve --env production",
    "build": "webpack --env production",
    "test": "test"
  },
  "overrides": {
    "autoprefixer": "10.4.5"
  },
  "author": "Jekin Gohel",
  "license": "ISC",
  "dependencies": {
    "@babel/cli": "^7.25.9",
    "@babel/core": "^7.25.9",
    "@babel/plugin-transform-runtime": "^7.25.9",
    "@babel/preset-env": "^7.25.9",
    "@babel/preset-react": "^7.25.9",
    "@babel/runtime": "^7.25.9",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^9.2.1",
    "eslint": "^8.57.1",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jest": "^28.8.3",
    "path": "^0.12.7",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "webpack": "^5.95.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.1.0"
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "react-app",
      "react-app/jest"
    ],
    "env": {
      "browser": true,
      "commonjs": true,
      "node": true,
      "es6": true,
      "jest": true
    },
    "parserOptions": {
      "ecmaVersion": 2018
    },
    "rules": {
      "no-console": "off",
      "strict": [
        "error",
        "global"
      ],
      "indent": [
        "warn",
        2,
        {
          "SwitchCase": 1
        }
      ],
      "curly": "warn",
      "switch-colon-spacing": [
        "error",
        {
          "after": true,
          "before": false
        }
      ],
      "quotes": [
        "error",
        "double"
      ],
      "no-trailing-spaces": "error",
      "no-multiple-empty-lines": [
        "error",
        {
          "max": 2
        }
      ],
      "semi": [
        "error",
        "never"
      ]
    }
  },
  "devDependencies": {
    "css-loader": "^7.1.2",
    "dotenv-webpack": "^8.1.0",
    "node-sass": "^9.0.0",
    "sass": "^1.80.3",
    "sass-loader": "^16.0.2",
    "style-loader": "^4.0.0"
  },
  "engines": {
    "node": ">=16.14.0",
    "npm": ">=8.4"
  }
}
